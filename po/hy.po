#: ../plugins/cputemp/cputemp.c:638
msgid "CPU Temperature"
msgstr "Պրոցեսորի ջերմաստիճանը"

#: ../plugins/cputemp/cputemp.c:640
msgid "Foreground colour"
msgstr "Դիմային ֆոնի գույնը"

#: ../plugins/cputemp/cputemp.c:641
msgid "Background colour"
msgstr "Հետևի ֆոնի գույնը"

#: ../plugins/cputemp/cputemp.c:642
msgid "Colour when ARM frequency capped"
msgstr "Գունավորել, երբ ARM հաճախականությունը սահմանային է"

#: ../plugins/cputemp/cputemp.c:643
msgid "Colour when throttled"
msgstr "Գունավորել, երբ ծանրաբեռնված է"

#: ../plugins/cputemp/cputemp.c:644
msgid "Lower temperature bound"
msgstr "Նվազագույն ջերմաստիճանի շեմ"

#: ../plugins/cputemp/cputemp.c:645
msgid "Upper temperature bound"
msgstr "Առավելագույն ջերմաստիճանի շեմ"

#: ../plugins/cputemp/cputemp.c:653
msgid "CPU Temperature Monitor"
msgstr "Պրոցեսորի ջերմաստիճանի վերահսկում"

#: ../plugins/cputemp/cputemp.c:655
msgid "Display CPU temperature"
msgstr "Ցույց տալ պրոցեսորի ջերմաստիճանը"
